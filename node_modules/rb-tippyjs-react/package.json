{
  "name": "rb-tippyjs-react",
  "version": "4.3.1",
  "description": "React component for Tippy.js",
  "main": "dist/tippy-react.umd.js",
  "module": "dist/tippy-react.esm.js",
  "unpkg": "dist/tippy-react.umd.min.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "parcel serve demo/index.html",
    "build": "rollup --config --bundleConfigAsCjs",
    "test": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.js\"",
    "format": "prettier --write \"{src,test,demo}/**/*.{js,ts,json,css,md}\""
  },
  "author": "atomiks",
  "license": "MIT",
  "keywords": [
    "tooltip",
    "popover",
    "tippy",
    "react"
  ],
  "files": [
    "dist/",
    "headless/",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReactBricks/tippyjs-react"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>test/setup.js",
      "@testing-library/jest-dom/extend-expect"
    ],
    "coveragePathIgnorePatterns": [
      "test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,demo}/**/*.{js,ts,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "{src,test}/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.26.9",
    "react-is": "^19.0.0",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "babel-plugin-styled-components": "^2.1.4",
    "core-js": "^3.41.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "framer-motion": "^10.18.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.3.0",
    "parcel": "^2.13.3",
    "prettier": "^3.5.3",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-spring": "^9.7.5",
    "rollup": "^3.29.5",
    "styled-components": "^5.3.11",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@babel/core": "^7.26.9",
    "react": "^19.0",
    "react-dom": "^19.0"
  },
  "packageManager": "yarn@3.5.1+sha256.64c0b63e5fa0e21f354b5eff7c9a1b486f32046bfc3283670569e3c6a9cad102"
}
